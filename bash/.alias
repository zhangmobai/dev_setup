alias ls='ls -F --color=auto'
alias ll='ls -ltrh -F --color=auto'
alias less='less -R'
alias grep='grep -n --color=auto'
alias c='clear'
alias d='git diff | colordiff | less -R'
alias dc='git diff --cached | colordiff | less -R'
alias m='make -j 12'
alias vim='vim -p'
alias dus='du --si -s *'
alias df='df -h'
alias master='git co master'
alias u='git pull && git submodule foreach git pull'
alias logp='git log --pretty=format:"%ar %an %s"'
alias cj='ctags -R --language-force=java *'
#alias mfm='git fetch && git merge origin/master && git reset --soft origin/master && git commit -a'
alias au='sudo apt-get update'
alias cm='cmake ..'
